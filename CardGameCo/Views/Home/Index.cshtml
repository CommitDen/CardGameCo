@model TimeSpan
<div class="text-center">
    <h1 class="display-4">@Model.TotalSeconds</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>

    @{ for (int i = 0; i < Model.TotalSeconds; i++)
        {
            <tr>
                <dt>Model.TotalSeconds</dt>
                <dt>Model.TotalHours</dt>
            <tr/>
        }
    }

   
</div>

